{
  "importOrder": [
    "^(assets|handlers|app|components|hoc|context|hooks|views|store|listeners|layouts|lib|pages|routes|services|styles|ui|utils)/?(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none"
}
